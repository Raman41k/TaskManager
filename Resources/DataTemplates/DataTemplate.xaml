<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:TaskManager.ViewModels"
                    xmlns:view="clr-namespace:TaskManager.Views">

    <DataTemplate DataType="{x:Type vm:AutostartsViewModel}">
        <view:AutostartsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProcessesViewModel}">
        <view:ProcessesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ServicesViewModel}">
        <view:ServicesView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ModulesViewModel}">
        <view:ModulesView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AboutViewModel}">
        <view:AboutView/>
    </DataTemplate>
</ResourceDictionary>